{% extends '@Wcm/Layout/layout.html.twig' %}



{% block main %}
    <h1>User</h1>
{{ dump() }}
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ User.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ User.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ User.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_wcm_users') }}">back to list</a>

    <a href="{{ path('app_wcm_edit_user', {'id': User.id}) }}">edit</a>

    {{ include('@Wcm/User/_delete_form.html.twig') }}
{% endblock %}
